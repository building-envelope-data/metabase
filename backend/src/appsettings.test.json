{
  "Logging": {
    "EnableSensitiveDataLogging": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Warning",
        "System": "Error"
      }
    }
  }
}
